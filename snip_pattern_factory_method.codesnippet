	<string>// Factory method pattern
+ (instancetype)objectWithType:(NSString *)type {
    if ([type isEqualToString:@"TypeA"]) {
        return [[ClassA alloc] init];
    } else if ([type isEqualToString:@"TypeB"]) {
        return [[ClassB alloc] init];
    } else {
        return [[self alloc] init];
    }
}

// Convenience factory methods
+ (instancetype)defaultObject {
    return [[self alloc] init];
}

+ (instancetype)objectWithConfiguration:(NSDictionary *)config {
    id object = [[self alloc] init];
    [object applyConfiguration:config];
    return object;
}</string>
