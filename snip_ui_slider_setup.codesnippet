	<key>IDECodeSnippetSummary</key>
	<string>UISlider with min/max values and continuous updates</string>
	<key>IDECodeSnippetTitle</key>
	<string>UI Slider Setup</string>
	<key>IDECodeSnippetUserSnippet</key>
	<true/>
	<key>IDECodeSnippetVersion</key>
	<integer>2</integer>
</dict>
</plist>