	<key>IDECodeSnippetContents</key>
	<string>// Batch delete request (iOS 9+)
NSFetchRequest *fetchRequest = [[NSFetchRequest alloc] initWithEntityName:@"EntityName"];

// Optional: Add predicate to delete specific objects
NSPredicate *predicate = [NSPredicate predicateWithFormat:@"timestamp < %@", oldDate];
[fetchRequest setPredicate:predicate];

NSBatchDeleteRequest *deleteRequest = [[NSBatchDeleteRequest alloc] initWithFetchRequest:fetchRequest];
deleteRequest.resultType = NSBatchDeleteResultTypeCount;

NSError *error = nil;
NSBatchDeleteResult *result = [self.managedObjectContext executeRequest:deleteRequest error:&error];

if (error) {
    NSLog(@"Batch delete error: %@", error.localizedDescription);
} else {
    NSNumber *deletedCount = result.result;
    NSLog(@"Deleted %@ objects", deletedCount);
    
    // Reset context after batch delete
    [self.managedObjectContext reset];
}</string>
